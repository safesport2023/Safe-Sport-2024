        },
        "signupAttributes": [
          "EMAIL",
          "custom:sport"
        ],
        "socialProviders": [],
        "stringAttributeConstraints": {
          "custom:sport": {
            "allowedValues": [
              "Soccer",
              "Basketball",
              "Football",
              "Tennis",
              "Swimming"
            ],
            "maxLength": 20,
            "minLength": 1
          }
        },
        "usernameAttributes": [
          "EMAIL"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito"
    }
